{
  "name": "solace-candidate-assignment",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "drizzle-orm": "0.44.7",
    "next": "16.0.1",
    "postgres": "3.4.7",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.17",
    "@types/node": "24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "dotenv": "17.2.3",
    "drizzle-kit": "0.31.6",
    "eslint": "9.39.1",
    "eslint-config-next": "16.0.1",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.3"
  },
  "scripts": {
    "build": "NEXT_TELEMETRY_DISABLED=1 && pnpm noTel && next build",
    "build:start": "next start",
    "noTel": "next telemetry disable",
    "repack": "del -f .next node_modules package-lock.json pnpm-lock.yaml && pnpm install",
    "start": "next dev --turbopack",
    "generate": "drizzle-kit generate",
    "migrate:up": "node ./src/db/migrate.mjs",
    "seed": "node --loader esbuild-register/loader -r esbuild-register ./src/db/seed/index.ts"
  }
}
